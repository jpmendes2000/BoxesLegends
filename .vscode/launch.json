{
    "version": "0.2.0",
    "configurations": [
        {
            "type": "java",
            "name": "Launch JavaFX App",
            "request": "launch",
            "mainClass": "com.BoxesLegends.Main",
            "vmArgs": "--module-path \"lib/javafx\" --add-modules javafx.controls,javafx.fxml",
            "classPaths": [
                "bin",
                "lib/jars_mssql/mssql-jdbc-12.10.1.jre11.jar",
                "lib/javafx/javafx.controls.jar",
                "lib/javafx/javafx.fxml.jar",
                "lib/javafx/javafx.graphics.jar"
            ]
        }
    ]
}